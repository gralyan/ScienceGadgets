<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.summaryPanel {
		width: 100%;
		height: 100%;
	}
	.problemDescription {
		height: 30%;
	}
	.varPanel {
		display: inline-block;
		vertical-align: top;
		width: 30%;
		height: 70%;
	}
	.eqPanel {
		display: inline-block;
		vertical-align: top;
		width: 69%;
		height: 70%;
		border-left: solid silver 1px;
	}
	</ui:style>
	<g:FlowPanel styleName="{style.summaryPanel}">
		<g:Label styleName="{style.problemDescription}" ui:field="problemDescription" />
		<g:FlowPanel styleName="{style.varPanel}" ui:field="varPanel"></g:FlowPanel>
		<g:FlowPanel styleName="{style.eqPanel}" ui:field="eqPanel"></g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 